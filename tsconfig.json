{
    "include": [ "src/**/*" ],
    "exclude": [ "node_modules/"],
    "compilerOptions": {
        "target": "es2024",
        "module": "commonjs",
        "declaration": true,
        "sourceMap": true,
        "outDir": "./dist/",
        "removeComments": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
}